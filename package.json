{
  "name": "vscode-dot-http-files",
  "displayName": ".http files",
  "description": "Language support for .http files.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "url": "https://github.com/andreventuravale/vscode-dot-http-files"
  },
  "author": {
    "name": "Andre Vale",
    "email": "andreventuravale@gmail.com"
  },
  "publisher": "AndreValeOutlook",
  "license": "MIT",
  "contributes": {
    "languages": [
      {
        "id": "http",
        "aliases": [
          "HTTP Files",
          "http"
        ],
        "extensions": [
          ".http"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "http",
        "scopeName": "source.http",
        "path": "./syntaxes/http.tmLanguage.json"
      }
    ]
  }
}